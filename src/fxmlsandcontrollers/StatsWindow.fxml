<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="341.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlsandcontrollers.ControllerStats">
   <children>
      <TableView fx:id="tableViewStats" layoutY="32.0" prefHeight="295.0" prefWidth="260.0">
        <columns>
          <TableColumn fx:id="colStatName" prefWidth="156.0" text="Stat Name" />
          <TableColumn fx:id="colStatNumber" minWidth="0.0" prefWidth="101.0" text="Number" />
        </columns>
      </TableView>
      <Label layoutX="14.0" layoutY="14.0" text="Statistics" />
      <Button fx:id="btnExit" layoutX="400.0" layoutY="300.0" mnemonicParsing="false" onAction="#handleExit" text="Exit" />
      <BarChart fx:id="barChart" layoutX="261.0" layoutY="57.0" prefHeight="200.0" prefWidth="189.0" title="Stat Totals">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
   </children>
  <stylesheets>

    <URL value="@StyleSheet.css" />

  </stylesheets>
</AnchorPane>
